{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "clean": "rm -rf ./dist",
    "compile": "npm run lint && npm run clean && node_modules/typescript/bin/tsc",
    "compile:watch": "node_modules/typescript/bin/tsc --watch",
    "dev": "node_modules/.bin/nodemon --watch 'src/**/*.ts index.ts' --ignore 'src/**/*.test.ts src/**/*.spec.ts' --exec 'ts-node' index.ts",
    "start": "npm run compile && node_modules/.bin/ts-node dist/index.js --scripts-prepend-node-path",
    "test": "node_modules/jest/bin/jest.js --detectOpenHandles",
    "test:coverage": "node_modules/jest/bin/jest.js --detectOpenHandles --coverage",
    "lint": "node_modules/.bin/eslint 'src/**/*.{ts,tsx,js,jsx,json,md}'",
    "lint:fix": "npm run lint --fix",
    "db:init": "node_modules/.bin/sequelize-cli init",
    "db:create": "node_modules/.bin/sequelize db:create",
    "db:drop": "node_modules/.bin/sequelize db:drop",
    "db:refresh": "node_modules/.bin/sequelize db:drop && node_modules/.bin/sequelize db:create && node_modules/.bin/sequelize-cli db:migrate && node_modules/.bin/sequelize-cli db:seed:all",
    "db:migrate": "node_modules/.bin/sequelize-cli db:migrate",
    "db:migrate:create": "node_modules/.bin/sequelize-cli migration:generate --name",
    "db:migrate:undo": "node_modules/.bin/sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "node_modules/.bin/sequelize-cli db:migrate:undo:all",
    "db:migrate:undo:all:name": "node_modules/.bin/sequelize-cli db:migrate:undo:all --to",
    "db:model:create": "node_modules/.bin/sequelize-cli model:generate --name",
    "db:seed:all": "node_modules/.bin/sequelize-cli db:seed:all",
    "db:seed:create": "node_modules/.bin/sequelize-cli seed:generate --name",
    "db:seed:name": "node_modules/.bin/sequelize-cli db:seed --seed",
    "db:seed:undo": "node_modules/.bin/sequelize-cli db:seed:undo",
    "db:seed:undo:all": "node_modules/.bin/sequelize-cli db:seed:undo:all",
    "db:seed:undo:name": "node_modules/.bin/sequelize-cli db:seed:undo --seed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mahbubzulkarnain/javascript-express-graphql-boilerplate.git"
  },
  "keywords": [],
  "author": "Mahbub Zulkarnain",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mahbubzulkarnain/javascript-express-graphql-boilerplate/issues"
  },
  "homepage": "https://github.com/mahbubzulkarnain/javascript-express-graphql-boilerplate#readme",
  "dependencies": {
    "apollo-datasource-rest": "^0.6.11",
    "apollo-server-errors": "^2.4.2",
    "apollo-server-express": "^2.18.2",
    "apollo-server-testing": "^2.18.2",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "dataloader": "^1.4.0",
    "express": "^4.17.1",
    "express-pino-logger": "^4.0.0",
    "firebase": "^7.24.0",
    "firebase-admin": "^8.13.0",
    "googleapis": "^44.0.0",
    "graphql": "^14.7.0",
    "graphql-response-parser": "0.0.5",
    "graphql-tag": "^2.11.0",
    "graphql-tools": "^4.0.8",
    "lodash": "^4.17.20",
    "pg": "^7.18.2",
    "pino": "^5.17.0",
    "pino-pretty": "^3.6.1",
    "subscriptions-transport-ws": "^0.9.18",
    "validator": "^13.1.17"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/express-pino-logger": "^4.0.2",
    "@types/jest": "^24.9.1",
    "@types/lodash": "^4.14.162",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "apollo-server": "^2.18.2",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "faker": "^4.1.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "sequelize": "^5.22.3",
    "sequelize-cli": "^5.5.1",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "uuid": "^3.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": ""
    }
  }
}
